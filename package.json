    "preview": "astro preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp"
  },
  "dependencies": {
    "@astrojs/mdx": "^4.2.1",
    "@astrojs/rss": "^4.0.11",
    "@astrojs/sitemap": "^3.3.0",
    "@astrojs/vue": "^5.0.7",
    "@deck.gl/core": "^9.1.8",
    "@deck.gl/layers": "^9.1.8",
    "@unocss/reset": "^65.5.0",
    "astro": "^5.5.4",
    "nprogress": "^0.2.0",
    "unocss": "^65.5.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@iconify/json": "^2.2.320",
    "@types/nprogress": "^0.2.3",
    "@vueuse/core": "^13.0.0",
    "bumpp": "^10.1.0",
    "es-toolkit": "^1.33.0",
    "eslint": "^9.23.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-format": "^1.0.1",
    "lint-staged": "^15.5.0",
    "prettier-plugin-astro": "^0.14.1",
    "simple-git-hooks": "^2.12.1"
