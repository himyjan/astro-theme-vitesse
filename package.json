{
  "name": "astro-theme-vitesse",
  "type": "module",
  "version": "1.2.0",
  "description": "A minimal, SEO-friendly portfolio and blog theme for Astro, supports Vue and UnoCSS.",
  "author": "",
  "license": "MIT",
  "keywords": [
    "astro-theme-vitesse",
    "astro",
    "blog",
    "starter",
    "template"
  ],
  "engines": {
    "node": ">=v18.17.1"
  },
  "scripts": {
